{
  "name": "html-midi-player",
  "version": "0.0.1",
  "author": "Ondřej Cífka",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "webpack"
  },
  "main": "dist/index.js",
  "targets": {
    "main": {
      "includeNodeModules": false,
      "engines": {
        "browsers": "supports audio-api and supports custom-elementsv1 and supports shadowdomv1 and supports async-functions"
      }
    }
  },
  "devDependencies": {
    "css-loader": "^4.2.1",
    "focus-visible": "^5.1.0",
    "raw-loader": "^4.0.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@magenta/music": "^1.19.0"
  },
  "alias": {
    "@magenta/music/es6/core": {
      "global": "core"
    },
    "@magenta/music/es6/protobuf": {
      "global": "protobuf"
    }
  }
