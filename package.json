{
  "name": "html-midi-player",
  "version": "0.0.1",
  "author": "Ondřej Cífka",
  "type": "module",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist/esm",
    "prestart": "rm -rf dist",
    "start": "rollup -c --watch --plugin dev"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "focus-visible": "^5.1.0",
    "rollup": "^2.26.4",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.26.10",
    "style-loader": "^1.2.1",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@magenta/music": "^1.19.0"
  },
  "alias": {
    "@magenta/music/es6/core": {
      "global": "core"
    },
    "@magenta/music/es6/protobuf": {
      "global": "protobuf"
    }
  }
}
